                <text:p text:style-name="h1n">Module One</text:p>
                <text:p text:style-name="p">paragraph1</text:p>
                <text:h text:style-name="h1n" text:level="1">Objectivism and Constructivism</text:h>
                <!--<text:p text:style-name="h1n">Module Two</text:p>-->
                <text:p text:style-name="h2">Topic Two Point One Not Numbered</text:p>
                <text:p text:style-name="h-learning">Learning outcomes X</text:p>
                <text:p text:style-name="h2">Topic Two Point Two Not Numbered</text:p>
                <text:p text:style-name="h-learning">Learning outcomes Y</text:p>
                <text:p text:style-name="h-Objective">Objectives</text:p>
                <text:p text:style-name="p">paragraph2</text:p>
                <text:p text:style-name="h1n">Module Three</text:p>
                <text:p text:style-name="h2n">Topic Three Point One</text:p>
                <text:p text:style-name="h2n">Topic Three Point Two</text:p>
                <text:p text:style-name="h-Activity">Activity 1.1</text:p>
                <text:p text:style-name="h3n">Sub-Topic Three Point Two Point One</text:p>
                <text:p text:style-name="h3n">Sub-Topic Three Point Two Point Two</text:p>
                <text:p text:style-name="h4n">Idea Three Point Two Point Two Point One</text:p>
                <text:p text:style-name="h3n">Sub-Topic Three Point Two Point Three</text:p>
                <text:p text:style-name="h2n">Topic Three Point Three</text:p>
            </utfx:source>
            <utfx:expected validate="no">
                <p>paragraph0</p>
                <h1><a name="test"><!--test--></a>1 Module One</h1>
                <p>paragraph1</p>
                <h1><a name="test"><!--test--></a>2 Objectivism and Constructivism</h1>
                <h2><a name="test"><!--test--></a>Topic Two Point One Not Numbered</h2>
                <h6><a name="test"><!--test--></a>Learning outcomes X</h6>
                <h2><a name="test"><!--test--></a>Topic Two Point Two Not Numbered</h2>
                <h6><a name="test"><!--test--></a>Learning outcomes Y</h6>
                <h6><a name="test"><!--test--></a>Objectives</h6>
                <p>paragraph2</p>
                <h1><a name="test"><!--test--></a>3 Module Three</h1>
                <h2><a name="test"><!--test--></a>3.1 Topic Three Point One</h2>
                <h2><a name="test"><!--test--></a>3.2 Topic Three Point Two</h2>
                <h6><a name="test"><!--test--></a>Activity 1.1</h6>
                <h3><a name="test"><!--test--></a>3.2.1 Sub-Topic Three Point Two Point One</h3>
                <h3><a name="test"><!--test--></a>3.2.2 Sub-Topic Three Point Two Point Two</h3>
                <h4><a name="test"><!--test--></a>3.2.2.1 Idea Three Point Two Point Two Point One</h4>
                <h3><a name="test"><!--test--></a>3.2.3 Sub-Topic Three Point Two Point Three</h3>
                <h2><a name="test"><!--test--></a>3.3 Topic Three Point Three</h2>
            </utfx:expected>
        </utfx:assert-equal>
    </utfx:test>
