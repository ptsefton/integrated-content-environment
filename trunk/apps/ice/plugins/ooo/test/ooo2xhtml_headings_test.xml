    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <!-- test numbering -->
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <utfx:test>
        <utfx:name>Numbering1</utfx:name>
        <utfx:assert-equal>
            <utfx:source validate="no">
                <text:p text:style-name="h1n">And now numbered...Module One</text:p>
            </utfx:source>
            <utfx:expected validate="no">
                <h1><a name="test"><!--test--></a>1 And now numbered...Module One</h1>
            </utfx:expected>
        </utfx:assert-equal>
    </utfx:test>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <!-- test numbering -->
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <utfx:test>
        <utfx:name>Numbering2</utfx:name>
        <utfx:assert-equal>
            <utfx:source validate="no">
                <text:p text:style-name="h1n">And now numbered...Module One</text:p>
                <text:p text:style-name="h1n">Module Two</text:p>
            </utfx:source>
            <utfx:expected validate="no">
                <h1><a name="test"><!--test--></a>1 And now numbered...Module One</h1>
                <h1><a name="test"><!--test--></a>2 Module Two</h1>
            </utfx:expected>
        </utfx:assert-equal>
    </utfx:test>
