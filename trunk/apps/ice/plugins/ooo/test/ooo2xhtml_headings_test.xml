                    <text:h text:style-name="h1n" text:level="1">Objectivism and Constructivism
                            <text:span text:style-name="T2">-</text:span> stuff</text:h>
                    <text:p text:style-name="p">paragraph2</text:p>
                    <table:table/>
                    <text:list>
                        <text:list-item>
                            <text:p text:style-name="li1b">list item 1</text:p>
                        </text:list-item>
                        <text:list-item>
                            <text:p text:style-name="li1b">list item 2</text:p>
                        </text:list-item>
                    </text:list>
                    <text:h text:style-name="h1n" text:level="1">Objectivism and Constructivism 2</text:h>
                    <text:p text:style-name="h2n">Topic Three Point Three</text:p>
                    <test.me/>
                </test-preceding>
            </utfx:source>
            <!-- <el>utfx-wrapper</el> -->
            <utfx:expected validate="no">
                <preceding.elements>
                    <el>test-preceding</el>
                    <el>text:h</el>
                    <el>text:p</el>
                    <el>text:p</el>
                    <el>text:p</el>
                    <el>table:table</el>
                    <el>table:table</el>
                    <el>text:h</el>
                    <el>text:p</el>
                    <el>table:table</el>
                    <el>text:list</el>
                    <el>text:h</el>
                    <el>text:p</el>
                    <el>test.me</el>
                </preceding.elements>
            </utfx:expected>
        </utfx:assert-equal>
    </utfx:test>
