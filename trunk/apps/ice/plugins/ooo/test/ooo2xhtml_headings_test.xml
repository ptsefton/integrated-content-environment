    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <utfx:test>
        <utfx:name>numbering with text:h containing sub elements</utfx:name>
        <utfx:assert-equal>
            <utfx:source validate="no">
                <text:p text:style-name="h1">Module A</text:p>
                <text:p text:style-name="p" text:level="1">paragraph0</text:p>
                <text:p text:style-name="h1n">Module One</text:p>
                <text:p text:style-name="p">paragraph1</text:p>                
                <text:h text:style-name="h1n" text:level="1">Objectivism and Constructivism<text:span text:style-name="T2"> </text:span> stuff</text:h>
            </utfx:source>
            <utfx:expected validate="no">
                <h1><a name="test"><!--test--></a>Module A</h1>
                <p>paragraph0</p>
                <h1><a name="test"><!--test--></a>1 Module One</h1>
                <p>paragraph1</p>
                <h1><a name="test"><!--test--></a>2 Objectivism and Constructivism <span class="T2"> </span> stuff</h1>
            </utfx:expected>
        </utfx:assert-equal>
    </utfx:test>
