        <utfx:name>test preceding</utfx:name>
        <utfx:assert-equal>
            <utfx:source validate="no">
                <test-preceding>
                    <text:h text:style-name="h1">Module A</text:h>
                    <text:p text:style-name="p" text:level="1">paragraph0</text:p>
                    <text:p text:style-name="h1n">Module One</text:p>
                    <text:p text:style-name="p">paragraph1</text:p>
                    <table:table/>
