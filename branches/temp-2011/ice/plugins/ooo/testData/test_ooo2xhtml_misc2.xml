<test name="Footnotes" normalize-white-spaces="true">
	<source>
	  <office:document-content>
	    <office:body>
		<office:text>
<text:p text:style-name="p">Footnote test document</text:p>
<text:p text:style-name="p"><text:bookmark-start text:name="bookmarkOne"/>Bookmark<text:bookmark-end text:name="bookmarkOne"/>.</text:p>
<text:p text:style-name="p">
  <text:note text:id="ftn1" text:note-class="footnote">
    <text:note-citation>1</text:note-citation>
	<text:note-body>
	<text:p text:style-name="Footnote">Footnote test <text:span text:style-name="T1">bold</text:span>
		 and 
		<text:span text:style-name="T2">lager</text:span>. <text:s/>
		<text:a xlink:type="simple" xlink:href="#bookmarkOne">Link</text:a>!
	</text:p>
	<text:list text:style-name="li1n">
	  <text:list-item>
	    <text:p text:style-name="li1n">Number list one</text:p>
	  </text:list-item>
	  <text:list-item>
	    <text:p text:style-name="li1n">two</text:p>
	  </text:list-item>
	</text:list>
    </text:note-body>
  </text:note>
</text:p>
      	</office:text>
	    </office:body>
	  </office:document-content>
	</source>
	<expected match="/html:html/html:body">
   	  <body>
		<p>Footnote test document</p>
		<p><a name="bookmarkOne"><!--bookmarkOne--></a>Bookmark.</p>
		<p><span class="footnote"><a class="footnote" href="#ftn1" name="ftn1-text">1</a>
            <span class="footnote-text">
                <a href="#ftn1-text" name="ftn1">1</a>
                Footnote test <span class="T1">bold</span> and <span class="T2">lager</span> .
                <a href="#bookmarkOne">Link</a> !
                <ol class="lin" style="list-style: decimal;">
                  <li>
                    <p>Number list one</p>
                  </li>
                  <li>
                    <p>two</p>
                  </li>
                </ol>
            </span>
        </span></p>
		<hr/>
		<p>
		<div style="font-size: .9em;">
		  <span class="footnote-defined"><a href="#ftn1-text" name="ftn1">1</a> Footnote test
				<span class="T1">bold</span> and <span class="T2">lager</span> . 
				<a href="#bookmarkOne">Link</a> !
			<ol class="lin" style="list-style: decimal;">
				<li><p>Number list one</p></li>
				<li><p>two</p></li>
			</ol>
		  </span>
		</div>
	    </p>
        </body>
	</expected>
</test>

<test name="image in pre" normalize-white-spaces="true">
	<source>
	  <office:document-content>
	    <office:body>
		<office:text>
<text:p text:style-name="pre1">
	<draw:frame draw:style-name="fr1" draw:name="graphics1" text:anchor-type="as-char" svg:width="8.509cm" svg:height="11.356cm" draw:z-index="0">
	  <draw:image xlink:href="Pictures/10000000000001B40000024632DC26C8.jpg" xlink:type="simple" xlink:show="embed" xlink:actuate="onLoad"/>
	</draw:frame>
</text:p>
<text:p text:style-name="p">
	<text:span text:style-name="i-b">Figure 1</text:span>
</text:p>
      	</office:text>
	    </office:body>
	  </office:document-content>
	</source>
	<expected match="/html:html/html:body">
   	  <body>
		<pre>
		  <a name="graphics1"><!-- --></a><img alt="graphics1" class="fr1" height="429" src="Pictures/10000000000001B40000024632DC26C8.jpg" style="border:0px; vertical-align: top" width="322"/>
		</pre>
		<p><b>Figure 1</b></p>
        </body>
	</expected>
</test>

