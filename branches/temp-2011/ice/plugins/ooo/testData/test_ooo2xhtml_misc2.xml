<test name="blockquote test" normalize-white-spaces="true">
	<source>
	  <office:document-content>
          <office:automatic-styles>
          </office:automatic-styles>
	    <office:body>
		  <office:text>
            <text:p text:style-name="bq1">
                Test text
                <text:a xlink:type="simple" xlink:href="http://diveintopython3.org/about.html">link</text:a>
                , and more text <text:span text:style-name="User_20_Entry">Ctrl-R</text:span>
                the end.
            </text:p>
      	  </office:text>
	    </office:body>
	  </office:document-content>
	</source>
	<expected match="/html:html/html:body">
   	  <body>
        <blockquote class="bq">
            <p>
                Test text
                <a href="http://diveintopython3.org/about.html">link</a>
                , and more text
                <span class="User_20_Entry">Ctrl-R</span>
                the end.
            </p>
        </blockquote>
      </body>
	</expected>
</test>


<test name="caption test" normalize-white-spaces="true">
	<source>
	  <office:document-content>
          <office:automatic-styles>
            <style:style style:name="P1" style:family="paragraph" style:parent-style-name="p">
              <style:paragraph-properties fo:margin-top="0.42cm" fo:margin-bottom="0cm"/>
            </style:style>
            <style:style style:name="P3" style:family="paragraph" style:parent-style-name="Illustration">
              <style:paragraph-properties fo:margin-top="0.212cm" fo:margin-bottom="0.212cm"/>
            </style:style>
            <style:style style:name="fr1" style:family="graphic" style:parent-style-name="Frame">
             <style:graphic-properties style:wrap="run-through" style:number-wrapped-paragraphs="no-limit" style:vertical-pos="from-top" style:horizontal-pos="from-left" style:horizontal-rel="char" fo:background-color="#ffffff" style:background-transparency="0%" fo:padding="0.026cm" fo:border="none">
              <style:background-image/>
             </style:graphic-properties>
            </style:style>
            <style:style style:name="fr2" style:family="graphic" style:parent-style-name="Graphics">
             <style:graphic-properties fo:margin-left="0cm" fo:margin-right="0cm" style:vertical-pos="top" style:vertical-rel="baseline" fo:background-color="#ffffff" style:background-transparency="0%" fo:padding-left="0.28cm" fo:padding-right="0.28cm" fo:padding-top="0.153cm" fo:padding-bottom="0.153cm" fo:border="none" style:mirror="none" fo:clip="rect(0cm, 0cm, 0cm, 0cm)" draw:luminance="0%" draw:contrast="0%" draw:red="0%" draw:green="0%" draw:blue="0%" draw:gamma="100%" draw:color-inversion="false" draw:image-opacity="100%" draw:color-mode="standard">
              <style:background-image/>
             </style:graphic-properties>
            </style:style>
          </office:automatic-styles>
	    <office:body>
		  <office:text>
            <text:p text:style-name="p">
             a
             <draw:frame draw:style-name="fr1" draw:name="Frame1" text:anchor-type="as-char"
                    svg:y="0cm" svg:width="10.499cm" svg:height="8.698cm" draw:z-index="0">
              <draw:text-box>
               <text:p text:style-name="P1">
                <draw:frame draw:style-name="fr2" draw:name="graphics1" text:anchor-type="as-char"
                        svg:width="10.506cm" svg:height="7.211cm" draw:z-index="1">
                 <draw:image xlink:href="pic.png"
                        xlink:type="simple" xlink:show="embed" xlink:actuate="onLoad"/>
                </draw:frame>
                Illustration
                <text:sequence text:ref-name="refIllustration0" text:name="Illustration"
                        text:formula="ooow:Illustration+1" style:num-format="1">1</text:sequence>
                : Caption Text
               </text:p>
              </draw:text-box>
             </draw:frame>
             testing
            </text:p>

            <text:p text:style-name="p">
             <draw:frame draw:style-name="fr1" draw:name="Frame2" text:anchor-type="as-char"
                    svg:y="0cm" svg:width="14.99cm" svg:height="7.116cm" draw:z-index="2">
              <draw:text-box>
               <text:p text:style-name="P3">
                <draw:frame draw:style-name="fr2" draw:name="graphics2" text:anchor-type="as-char"
                        svg:width="15.004cm" svg:height="6.346cm" draw:z-index="3">
                 <draw:image xlink:href="pic2.png" xlink:type="simple" xlink:show="embed"
                        xlink:actuate="onLoad"/>
                </draw:frame>
                Illustration
                <text:sequence text:ref-name="refIllustration1" text:name="Illustration"
                        text:formula="ooow:Illustration+1" style:num-format="1">2</text:sequence>
                : Another caption
               </text:p>
              </draw:text-box>
             </draw:frame>
            </text:p>
          </office:text>
	    </office:body>
	  </office:document-content>
	</source>
	<expected match="/html:html">
     <html xmlns="http://www.w3.org/1999/xhtml">
      <head>
       <meta content="text/html; charset=UTF-8" http-equiv="Content-Type"/>
       <title>Untitled</title>
       <style type="text/css">table {border-spacing: 0;empty-cells: show; }
        .body .indent {margin-left: 25px;} 
        .body .hint {font-size: 1.4em; font-style: normal;font-weight:bolder;color: #00000;}
        .spCh {font-family: 'Lucida Grande','Arial Unicode MS', sans-serif; font-size: 1.2em;}
        span.underline {text-decoration:underline; }
        p.P1 {margin-bottom:0cm; margin-top:0.42cm; }
        Illustration.P3 {margin-bottom:0.212cm; margin-top:0.212cm; }
        Frame.fr1 {background-color:#ffffff; border:none; padding:0.026cm; }
        img.fr2 {background-color:#ffffff; border:none; margin-left:0cm; margin-right:0cm;
        padding-bottom:0.153cm; padding-left:0.28cm; padding-right:0.28cm; padding-top:0.153cm; }</style>
      </head>
   	  <body>
       <p>
         a
         <p class="P1" style="width:397px;">
             <a name="graphics1"><!-- --></a>
             <img alt="graphics1" class="fr2" height="273" src="pic.png"
                    style="border:0px; vertical-align: top" width="397"/>
             Illustration 1 : Caption Text
         </p>
         testing
       </p>

       <p>
        <p class="Illustration.P3" style="margin-bottom:0.212cm; margin-top:0.212cm; width:567px;">
          <a name="graphics2"><!-- --></a>
          <img alt="graphics2" class="fr2" height="240" src="pic2.png"
                  style="border:0px; vertical-align: top" width="567"/>
            Illustration 2 : Another caption
        </p>
       </p>
      </body>
     </html>
	</expected>
</test>

